<script
  src="https://code.jquery.com/jquery-3.7.1.min.js"
  integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo="
  crossorigin="anonymous"
></script>

<script>
  function updateServiceVisibility(serviceClass, radioButtonId) {
    const serviceElements = document.getElementsByClassName(serviceClass);
    const showRadio = document.getElementById(radioButtonId);

    Array.from(serviceElements).forEach((el) => {
      el.style.display = showRadio.checked ? '' : 'none';
    });

    const serviceMChecked = document.getElementById('serviceM').checked;
    const serviceGChecked = document.getElementById('serviceG').checked;

    if ($('#serviceM').is(':checked') || $('#serviceG').is(':checked')) {
      jQuery('#btn-save-estimation').attr('disabled', false);
      $('.error').hide();
    } else {
      $('.both').hide();
      $('.error').show();
      jQuery('#btn-save-estimation').attr('disabled', true);
    }
  }

  // Event listeners
  document.getElementById('serviceG').addEventListener('change', () => updateServiceVisibility('serviceG', 'serviceG'));
  document.getElementById('serviceM').addEventListener('change', () => updateServiceVisibility('serviceM', 'serviceM'));

  // Initial visibility updates
  updateServiceVisibility('serviceG', 'serviceG');
  updateServiceVisibility('serviceM', 'serviceM');
</script>

<script>
  function submitFormData(formElement) {
    const formData = new FormData(formElement);
    const formObject = {};
    formData.forEach((value, key) => {
      formObject[key] = value;
    });
    formObject['customerId'] = '{{ customer.id }}';

    if($('#serviceG').is(':checked') && !$('#serviceM').is(':checked')){
      formObject['aoi_inspection']=null;
      formObject['beaten_thickness']=null;
      formObject['board']=null;
      formObject['board_thickness']=null;
      formObject['build_up_method']=null;
      formObject['chamfering_process']=null;
      formObject['data_service']=null;
      formObject['dip_cnt']=null;
      formObject['dxf_data']=null;
      formObject['impedance']=null;
      formObject['jumpvcut_cnt']=null;
      formObject['laser_via']=null;
      formObject['local_storage_mapkey']=null;
      formObject['long_hole']=null;
      formObject['manu_opt_plan_cad']=null;
      formObject['manufacture_Inspection_certificate']=null;
      formObject['min_hole_land']=null;
      formObject['org_spec_summary_order_no']=null;
      formObject['min_ls']=null;
      formObject['other_silk']=null;
      formObject['pad_less_03']=null;
      formObject['pad_on_via']=null;
      formObject['parts_height_limit']=null;
      formObject['parts_height_limit']=null;
      formObject['resist_color']=null;
      formObject['rf_contact_portion_1']=null;
      formObject['rf_contact_portion_bonding_1']=null;
      formObject['rf_contact_portion_bonding_2']=null;
      formObject['rf_contact_portion_cnt']=null;
      formObject['roulette']=null;
      formObject['roulette_kind']=null;
      formObject['router_cut']=null;
      formObject['router_cut_kind']=null;
      formObject['semi_th']=null;
      formObject['silk']=null;
      formObject['silk_color']=null;
      formObject['silk_print_method']=null;
      formObject['surface_finishing']=null;
      formObject['ul_mark']=null;
      formObject['vcut_cnt']=null;
    }else if(!$('#serviceG').is(':checked') && $('#serviceM').is(':checked')){
        formObject['freeMaterialsFlg']=null;
    }
    
    if(formObject.serviceD==undefined){
      formObject['serviceD'] = '0';
    }
    if(formObject.serviceG==undefined){
      formObject['serviceD'] = '0';
    }
   
 
    $("#btn-save-estimation").prop('disabled', true);
    $("#btn-save-estimation").html('saving...')
    fetch(appURL + '/api/draft-orders', {
      method: 'POST',
      mode: 'cors',
      headers: {
        'Content-Type': 'application/json',
        'bypass-tunnel-reminder': '104.181.194.237',
        'User-Agent': 'MyCustomUserAgent/1.0',
        'ngrok-skip-browser-warning': 'anyvalue',
      },
      body: JSON.stringify(formObject),
    })
      .then((response) => {
        $("#btn-save-estimation").prop('disabled', false);
        $("#btn-save-estimation").html('Save and Order →')
        return response.json().then((data) => {
          return { status: response.status, data };
        });
      })
      .then(({ status, data }) => {
        if (status === 400) {
          if (data.errors) {
            document.querySelectorAll('.inline-error').forEach(el => {
              el.style.display = 'none';
              el.innerHTML = '';
          });
            let errorMessages = '<h2 class="inline-error inline-error-title">Please fix the following errors:</h2><ul>';
            let firstElement="";
            for (const [key, value] of Object.entries(data.errors)) {
              console.log(value,"value")
              const object_value = value?.split(":");
              const labelPreFix = object_value[0];
              errorMessages += `<li style="font-size:12px;" class="uppercase">${value.replace(":",": ").replaceAll("_"," ")}</li>`;

              const errorElement = document.getElementById(labelPreFix + "_error");
              if (errorElement) {
                if(firstElement==""){
                  firstElement=errorElement;
                }
                
                  errorElement.innerHTML =object_value[1];
                  errorElement.style.display = "block"; // Make it visible
              } else {
                  console.error("Element not found:", labelPreFix + "_error");
              }
            }
            errorMessages += '</ul>';
            document.querySelectorAll('.messagebar').forEach(function (element) {
              element.innerHTML = errorMessages;
            });

            if (firstElement) {
              window.scrollTo({
                top: firstElement.getBoundingClientRect().top + window.scrollY - 150, // Scroll 50px above
                behavior: 'smooth'
              });
            }
          }
        } else if (status === 200) {
          if (data.shopifyOrderId) {
            const orderStatusURL = `/pages/estimation-detail?id=${data.shopifyOrderId}&step=2`;
            window.location.href = orderStatusURL;
          }
        }
      })
      .catch((error) => {
        console.error('Error:', error);
      });
  }

  // Handle form submission
  document.getElementById('pcb_calculator').addEventListener('submit', function (event) {
    event.preventDefault();
    const customerId = '{{ customer.id }}';
    const myModal = new bootstrap.Modal('#loginModal', {
      keyboard: false,
    });

    // Check if the user is logged in
    if (!customerId || customerId === '') {
      const formData = new FormData(this);
      const formObject = {};
      formData.forEach((value, key) => {
        formObject[key] = value;
      });
      localStorage.setItem('formData', JSON.stringify(formObject));
      myModal.show();
      return false;
    }
    submitFormData(this);
  });
  document.addEventListener('DOMContentLoaded', function () {
    const formElement = document.getElementById('pcb_calculator');
    const savedFormData = localStorage.getItem('formData');

    if (savedFormData) {
      const formObject = JSON.parse(savedFormData);

      // Fill form fields with saved data
      for (const [key, value] of Object.entries(formObject)) {
        const inputElement = formElement.querySelector(`[name="${key}"]`);

        if (inputElement) {
          inputElement.value = value;
        }
      }
      document.querySelectorAll(".grey-out").forEach((element) => {
        element.style.display = "none";
      });
      localStorage.removeItem('formData');
      // Automatically submit the form after login
      //submitFormData(formElement);
      // Clear form data from localStorage after submission
      
    }
  });
</script>

<script>
  $(document).ready(function () {
    $('.serviceD').hide();
    $('.serviceG').hide();

    $('#serviceG').change(function () {
      if ($(this).is(':checked')) {
        $('.serviceG').show();
      } else {
        $('.serviceG').hide();
      }
    });
  
    $('#serviceM').change(function () {
      if ($(this).is(':checked')) {
        $('.serviceM').show();
      } else {
        $('.serviceM').hide();
      }
    });
  });

  const defaultValuesServiceM = {
    kiban_type: '1',
    layer: '2',
    board: '0',
    board_thickness: '0.15',
    alm_thickness: null,
    beaten_thickness: '1',
    build_up_method: null,
    min_ls: '0.075',
    min_hole_land: '0',
    impedance: '0',
    resist_print: '1',
    resist_color: '0',
    silk: '2',
    silk_color: '0',
    silk_print_method: '0',
    surface_finishing: '0',
    chamfering_process: '0',
    router_cut: '0',
    roulette: '0',
    data_service: '0',
    semi_th: '0',
    long_hole: '0',
    pad_on_via: '0',
    pad_less_03: '0',
    ul_mark: '0',
    aoi_inspection: '0',
    manufact_inspection_certificate: '0',
    dxf_data: '0',
    manu_opt_plan_cad: '0',
    hard_gold_on_pci_edge_terminal: '0',
    perforation: '0',
    factory_id: '0',
    mount_inspection_certificate: '0',
  };
  const $form = $('#pcb_calculator');
  $.each(defaultValuesServiceM, function (key, value) {
    const $inputElement = $form.find(`[name="${key}"]`);
    if ($inputElement.length) {
      if ($inputElement.is(':radio')) {
        $inputElement.each(function () {
          if ($(this).val() === value) {
            $(this).prop('checked', true);
          } else {
            $(this).prop('checked', false); 
          }
        });
      } else {
        $inputElement.val(value);
      }
    }
  });

  const serviceBoxes = document.querySelectorAll('.service-child');
  serviceBoxes.forEach((serviceBox) => {
    const checkbox = serviceBox.querySelector('input[type="checkbox"]');
    if (checkbox) {
      checkbox.addEventListener('change', () => {
        if (checkbox.checked) {
          serviceBox.classList.add('checked');
        } else {
          serviceBox.classList.remove('checked');
        }
      });
    }
  });

  document.querySelectorAll('#pcb_calculator input').forEach((input) => {
    input.addEventListener('input', handleInputChange);
  });
  function handleInputChange() {
    const form = document.getElementById('pcb_calculator');
    const formData = new FormData(form);
    const layerValue = formData.get('layer');
    const rogersMaterial = formData.get('board');
    const size_x = formData.get('size_x')
    const size_y = formData.get('size_y')
    const board = formData.get('board')
    
    console.log(board, "Exteranal board")


    document.getElementById('patternSide-container').style.display = layerValue === '1' ? '' : 'none';

    const rogersElements = document.querySelectorAll('.rogers-only');
    rogersElements.forEach((element) => {
      element.style.display = rogersMaterial === '7' ? '' : 'none';
    });
    
    if (size_x >= 1 && size_y>=1) {
      document.querySelectorAll(".grey-out").forEach((element) => {
          element.style.display = "none";
      });
    }else{
      document.querySelectorAll(".grey-out").forEach((element) => {
        element.style.display = "block";
      });
    }
    const radioButtons = document.querySelectorAll('input[name="board_thickness"]');
    if (board == 2) {
      radioButtons.forEach((radio) => {
          if (!["1.0", "1.6"].includes(radio.value)) {
              const parentLabel = radio.closest('label'); // Select the closest label
              if (parentLabel) {
                  parentLabel.style.display = "none"; // Hide the label
              }
          }
      });
    }else{
      radioButtons.forEach((radio) => {

            const parentLabel = radio.closest('label'); // Select the closest label
            if (parentLabel) {
                parentLabel.style.display = "flex"; // Hide the label
            }

    });
    }
  }
</script>
